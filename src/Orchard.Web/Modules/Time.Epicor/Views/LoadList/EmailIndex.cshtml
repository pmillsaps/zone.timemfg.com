@model IEnumerable<Time.Data.EntityModels.TimeMFG.LoadListEmail>

@{
    ViewBag.Title = "Email Address List";
}

<h2>@ViewBag.Title</h2>

<hr />

<ul class="list-inline">
    <li>@Html.ActionLink("LoadList Home Page", "Index", null, new { @class = "btn btn-primary" })</li>
    <li>@Html.ActionLink("Add New Email", "EmailCreate", null, new { @class = "btn btn-primary" })</li>
</ul>

<hr />

<table class="table table-striped table-bordered table-condensed">
    <tr class="success">
        <th></th>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.ActionLink("Delete", "EmailDelete", new { id = item.Id }, new { @class = "delete16image" })
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.ActionLink(item.Email, "EmailEdit", new { id = item.Id })
                @*@Html.DisplayFor(modelItem => item.Email)*@
            </td>
        </tr>
    }
</table>