@model IEnumerable<Time.Data.EntityModels.Configurator.ConfigOption3>

@{
    ViewBag.Title = "Config Option 3 Index";
}

<h2>@ViewBag.Title</h2>

<div align="right">
    <p>
        @Html.ActionLink("Back to Configurator Index", "Index", "Configurator")
    </p>
</div>

<p>
    @Html.ActionLink("Create New Option", "Create")
</p>
<table class="table table-bordered table-condensed table-striped">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ConfigName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ConfigData)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Key1)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Key2)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Key3)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ConfigOption)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Inactive)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ConfigName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ConfigData)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Key1)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Key2)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Key3)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ConfigOption)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Inactive)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>

<div align="right">
    <p>
        @Html.ActionLink("Back to Configurator Index", "Index", "Configurator")
    </p>
</div>