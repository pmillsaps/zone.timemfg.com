@model Time.Data.EntityModels.Configurator.ComplexStructure

@{
    ViewBag.Title = "Complex Structure Details";
}

<h2>@ViewBag.Title</h2>

<div>
    <h4>Complex Structure</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.ConfigName)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.ConfigName)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.ConfigData)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.ConfigData)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Sequence)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Sequence)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.LookupData)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.LookupData)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.LookupSeq)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.LookupSeq)
        </dd>
    </dl>
</div>
<p>
    @Html.ActionLink("Edit", "Edit", new { id = Model.Id}) |
    @Html.ActionLink("Back to List", "Index", new { ConfigNames = Model.ConfigName, ConfigData = Model.ConfigData })
</p>

@using (Html.BeginForm(new { Search = ViewBag.Search }))
{
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true, "", new { @class = "text-danger" })

    <div class="row">
        <div class="col-md-1">@Html.Label("Search :", htmlAttributes: new { @class = "lead" })</div>
        <div class="col-md-5">
            @Html.TextBox("Search")
        </div>
        <div><input type="submit" value="Search" class="btn btn-primary" /></div>
    </div>
}

@Html.Action("_LookupMatrix", new { id = Model.Id })

@Html.Partial("_CriteriaDescriptions")

@Html.Action("_ComplexLinkMatrix", new { id = Model.Id, Search = ViewBag.Search })