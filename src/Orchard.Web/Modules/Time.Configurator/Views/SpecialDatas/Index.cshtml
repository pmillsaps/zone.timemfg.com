@model IEnumerable<Time.Data.EntityModels.Configurator.SpecialData>

@{
    ViewBag.Title = "Special Data Index";
}

<div align="right">
    <p>
        @Html.ActionLink("Back to Configurator Index", "Index", "Configurator")
    </p>
</div>

<h2>@ViewBag.Title</h2>

<p>
    @Html.ActionLink("Create New Special Data", "Create")
</p>
<table class="table table-striped table-bordered table-condensed">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Part)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Quantity)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Price)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SpecialConfig.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SpecialDataType.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SpecialRelatedOp.Operation)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Part)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Quantity)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Price)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SpecialConfig.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SpecialDataType.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SpecialRelatedOp.Operation)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>

<div align="right">
    <p>
        @Html.ActionLink("Back to Configurator Index", "Index", "Configurator")
    </p>
</div>