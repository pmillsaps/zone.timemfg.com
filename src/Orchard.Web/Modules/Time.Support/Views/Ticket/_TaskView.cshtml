@model Time.Data.EntityModels.TimeMFG.TicketTask

<div class="row">
    <div class="col-md-2">
        @Html.DisplayFor(m => m.Completed)
        @*@Html.CheckBox("Completed", item.Completed == true, new { @Enabled = false })*@
    </div>
    <div class="col-md-10">
        @Html.ActionLink(Model.Task, "EditTask", new { id = Model.ID })
        @*@Html.DisplayFor(m => item.Task)*@
    </div>
</div>
<div class="row">
    <div class="col-md-2">
        @Html.DisplayFor(m => m.TicketEmployee.FullName)
    </div>
    <div class="col-md-10">
        @Html.DisplayWithBreaksFor(m => m.Notes)
    </div>
</div>
<hr class="col-md-12" />
<div class="row">&nbsp;</div>