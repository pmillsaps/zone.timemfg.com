@model IEnumerable<Time.Data.EntityModels.DataPlates.DielectricRating>

@{
    ViewBag.Title = "Dielectric Rating Index";
}

<h2>@ViewBag.Title</h2>

<div align="left">
    <p>
        @Html.ActionLink("Back to DataPlates Index", "Index", "Home")
    </p>
</div>

@*<p>
        @Html.ActionLink("Create New Dielectric Rating", "Create")
    </p>*@

<table class="table table-striped table-bordered table-condensed">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.SearchOrder)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SearchString)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Rating)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LineVoltage)
        </th>
        @*<th></th>*@
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.SearchOrder)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SearchString)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Rating)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LineVoltage)
            </td>
            @*<td>
                @Html.ActionLink("Edit", "Edit", new { id = item.SearchOrder }) |
                @Html.ActionLink("Details", "Details", new { id = item.SearchOrder }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.SearchOrder })
            </td>*@
        </tr>
    }

</table>

<div align="left">
    <p>
        @Html.ActionLink("Back to DataPlates Index", "Index", "Home")
    </p>
</div>