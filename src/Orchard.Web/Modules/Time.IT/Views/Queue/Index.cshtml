@model IEnumerable<Time.Data.Models.MessageQueue.MessageView>

@{
    ViewBag.Title = "Message Queue Contents : " + DateTime.Now;
}
<div class="col-md-12">
    <h2>@ViewBag.Title</h2>

    <table class="table table-striped table-bordered table-condensed">
        <tr>

            <th>
                @Html.DisplayNameFor(model => model.Label)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ArrivedTime)
            </th>
        </tr>

        @foreach (var item in Model)
            {
            <tr>

                <td>
                    @Html.DisplayFor(modelItem => item.Label)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Id)
                </td>
                <td>
                    @item.ArrivedTime.ToShortDateString() @item.ArrivedTime.ToShortTimeString()
                </td>
            </tr>
        }
    </table>
</div>
@{Html.RenderAction("_ActiveQueue");}

@{Html.RenderAction("_Processes");}