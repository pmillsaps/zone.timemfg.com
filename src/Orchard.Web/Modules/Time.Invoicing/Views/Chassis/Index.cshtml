@model IEnumerable<Time.Invoicing.EntityModels.PcInvoice.Chassis>
@{
    ViewBag.Title = "Index";
}
<h2>
    Index</h2>
@if (!String.IsNullOrEmpty(ViewBag.ErrorMessage))
{
    <div class="error">@ViewBag.ErrorMessage</div>
}
<p>
    @Html.ActionLink("Create New Chassis", "Create")
</p>
<table class="table table-striped table-bordered table-condensed">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Inactive)
        </th>
        <th>
            &nbsp;
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FullName)
        </th>
        <th>
        </th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td class="align-center">
                @Html.DisplayFor(modelItem => item.Inactive)
            </td>
            <td>
                &nbsp;
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FullName)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.Id })
            </td>
        </tr>
    }
</table>