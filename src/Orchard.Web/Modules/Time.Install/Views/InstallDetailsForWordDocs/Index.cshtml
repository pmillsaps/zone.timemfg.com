@model IEnumerable<Time.Data.EntityModels.Install.InstallDetailsForWordDoc>

@{
    ViewBag.Title = "Installation Details for the Quotation Presentation";
}

<link href="~/Modules/Time.IT/Styles/customStyle.css" rel="stylesheet" />

<h2>@ViewBag.Title</h2>
<div align="right">
    @Html.ActionLink("Back to Main Menu", "Index", "Home")
</div>
<hr />


<div>
    <a style="margin-bottom:5px;" class="btn btn-primary btn-xs" href="@Url.Action("Create", "InstallDetailsForWordDocs")">Create New Lift Family</a>
</div>

<table class="table table-bordered table-hover table-responsive table-striped">
    <tr class="success">
        <th>
            @Html.DisplayNameFor(model => model.DetailLine)
        </th>
        <th class="col-md-1"></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DetailLine)
            </td>
            <td>
                <a href="@Url.Action("Edit", "InstallDetailsForWordDocs", new { id = item.Id })"><i class="fa fa-pencil-square-o" aria-hidden="true"></i></a>

                <a href="@Url.Action("Details", "InstallDetailsForWordDocs", new { id = item.Id })"><i class="fa fa-eye" aria-hidden="true"></i></a>

                <a href="@Url.Action("Delete", "InstallDetailsForWordDocs", new { id = item.Id })"><i class="fa fa-trash-o" aria-hidden="true"></i></a>
            </td>
        </tr>
    }

</table>
